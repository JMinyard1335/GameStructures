; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="GameDesign"
config/features=PackedStringArray("4.5", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Utils="*res://autoloads/utils.gd"
TileManager="*res://autoloads/tile_manager.gd"

[editor_plugins]

enabled=PackedStringArray("res://addons/gltf2meshlib/plugin.cfg", "res://addons/gut/plugin.cfg")

[importer_defaults]

animation_library={
&"animation/trimming": true,
&"fbx/allow_geometry_helper_nodes": true,
&"materials/extract": 1,
&"materials/extract_path": "res://assets"
}
